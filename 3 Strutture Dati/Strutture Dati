Module Program
    Sub Main()
        ' Lista (List)
        Dim listaNumeri As New List(Of Integer)()
        listaNumeri.Add(1)
        listaNumeri.Add(2)
        listaNumeri.Add(3)
        Console.WriteLine("Lista:")
        For Each numero In listaNumeri
            Console.WriteLine(numero)
        Next

        ' Dizionario (Dictionary)
        Dim dizionario As New Dictionary(Of String, String)()
        dizionario.Add("Italia", "Roma")
        dizionario.Add("Francia", "Parigi")
        dizionario.Add("Germania", "Berlino")
        Console.WriteLine("Dizionario:")
        For Each paese In dizionario
            Console.WriteLine(paese.Key & ": " & paese.Value)
        Next

        ' Coda (Queue)
        Dim coda As New Queue(Of String)()
        coda.Enqueue("Primo")
        coda.Enqueue("Secondo")
        coda.Enqueue("Terzo")
        Console.WriteLine("Coda:")
        While coda.Count > 0
            Console.WriteLine(coda.Dequeue())
        End While

        ' Pila (Stack)
        Dim pila As New Stack(Of String)()
        pila.Push("Primo")
        pila.Push("Secondo")
        pila.Push("Terzo")
        Console.WriteLine("Pila:")
        While pila.Count > 0
            Console.WriteLine(pila.Pop())
        End While

        ' Array
        Dim arrayNumeri As Integer() = {1, 2, 3, 4, 5}
        Console.WriteLine("Array:")
        For Each numero In arrayNumeri
            Console.WriteLine(numero)
        Next

        Console.ReadKey()
    End Sub
End Module
